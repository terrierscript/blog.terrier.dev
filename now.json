{
  "version": 2,
  "routes": [
    {
      "src": "/blog/(?<year>201[89])?(?<splat>[01].+)",
      "headers": {
        "Location": "/blog/$year/$year$splat"
      },
      "status": 301
    },
    {
      "src": "/feed/qiita_org",
      "dest": "https://qiita.com/terrierscript/feed.atom",
      "status": 200,
      "headers": {
        "Referer": ""
      },
      "continue": false
    },
    {
      "src": "/feed/qiita",
      "dest": "/api/feed/qiita",
      "status": 200
    },
    {
      "src": "/feed/devto",
      "dest": "https://dev.to/feed/terrierscript",
      "status": 200
    },
    {
      "src": "/feed/medium",
      "dest": "https://medium.com/feed/@terrierscript",
      "status": 200
    },
    {
      "src": "/feed/scrapbox",
      "dest": "https://scrapbox.io/api/feed/terrierscript/",
      "status": 200
    },
    {
      "src": "/feed/note",
      "dest": "https://note.mu/terrierscript/rss",
      "status": 200
    },
    {
      "src": "/blog/2020/20200906205259-next-js-avoiding-ssr-with-dynamic-import-ssr/",
      "dest": "https://zenn.dev/terrierscript/articles/2020-09-06-next-js-ssr-dynamic-import",
      "status": 301
    },
    {
      "src": "/blog/2020/20200901232406-string-string/",
      "dest": "https://zenn.dev/terrierscript/articles/2020-09-01-string-string.md",
      "status": 301
    }
  ],
  "env": {
    "SLACK_WEBHOOK_URL": "@slack_webhook_url"
  },
  "regions": [
    "hnd1"
  ],
  "github": {
    "silent": true
  }
}