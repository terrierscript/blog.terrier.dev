{
  "name": "snippet.terrierscript.com",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/terrierscript/snippets.terriercript.com",
  "license": "MIT",
  "private": true,
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "dependencies": {
    "lerna": "3.13.1",
    "netlify-lambda": "1.4.2",
    "typescript": "3.3.3333"
  },
  "scripts": {
    
    "build:functions": "netlify-lambda build ./packages/blog-functions",
    "build": "yarn workspace blog-core build",
    "_build": "yarn workspace blog-core build && yarn build:functions",
    "serve-function": "yarn dotenv netlify-lambda serve functions",
    "serve": "yarn workspace blog-core dev",
    "sandbox": "yarn workspace blog-sandbox start",
    "blog": "yarn workspace blog-bin blog"
  }
}
